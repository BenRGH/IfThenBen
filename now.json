{
    "version": 2,
    "public": true,
    "headers": [
        {
            "source": "/*.*",
            "headers": [
                {
                    "key": "X-Clacks-Overhead",
                    "value": "GNU Terry Pratchett"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}